{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "manual_typings"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "lodash/*": [
        "node_modules/@types/lodash-es/*"
      ],
      "@cfstratos/ajsf-core": [
        "dist/@cfstratos/ajsf-core"
      ],
      "@cfstratos/ajsf-core/*": [
        "dist/@cfstratos/ajsf-core/*"
      ],
      "@ajsf/bootstrap3": [
        "dist/@ajsf/bootstrap3"
      ],
      "@ajsf/bootstrap3/*": [
        "dist/@ajsf/bootstrap3/*"
      ],
      "@ajsf/bootstrap4": [
        "dist/@ajsf/bootstrap4"
      ],
      "@ajsf/bootstrap4/*": [
        "dist/@ajsf/bootstrap4/*"
      ],
      "@cfstratos/ajsf-material": [
        "dist/@cfstratos/ajsf-material"
      ],
      "@cfstratos/ajsf-material/*": [
        "dist/@cfstratos/ajsf-material/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}